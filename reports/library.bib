file = {:C$\backslash$:/Users/eribu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Howald et al. - 2007 - Invasive rodent eradication on islands.pdf:pdf},
isbn = {1523-1739 (Electronic)$\backslash$n0888-8892 (Linking)},
issn = {08888892},
journal = {Conservation Biology},
keywords = {Eradication,Invasive species,Island conservation,Mus musculus,Rattus exulans,Rattus norvegicus,Rattus rattus,t{\aa}ngedal},
mendeley-tags = {t{\aa}ngedal},
