pages = {113--9},
pmid = {11372940},
title = {{Outcome after total hip arthroplasty: Part II. Disease-specific follow-up and the Swedish National Total Hip Arthroplasty Register.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11372940},
volume = {72},
year = {2001}
