pmid = {25285615},
title = {{Traditions and myths in hip and knee arthroplasty}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4259040{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {85},
