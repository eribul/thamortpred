pmid = {20664749},
title = {{Within-cluster resampling for analysis of family data: Ready for prime-time?}},
url = {http://www.intlpress.com/site/pub/pages/journals/items/sii/content/vols/0003/0002/a004/ http://www.ncbi.nlm.nih.gov/pubmed/20963733},
