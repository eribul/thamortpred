doi = {10.1198/016214504000000845},
file = {:C$\backslash$:/Users/eribu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tian, Zucker, Wei - 2005 - On the Cox model with time-varying regression coefficients.pdf:pdf},
