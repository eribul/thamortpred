file = {:C$\backslash$:/Users/eribu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mair, Hatzinger, Maier - 2007 - Extended Rasch Modeling The R Package eRm.pdf:pdf},
