file = {:C$\backslash$:/Users/eribu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhang et al. - 2012 - An exploratory study of response shift in health-related quality of life and utility assessment among patients wit.pdf:pdf},
