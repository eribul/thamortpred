file = {:C$\backslash$:/Users/eribu/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lawrence et al. - 2019 - Difference in Restricted Mean Survival Time Small Sample Distribution and Asymptotic Relative Efficiency.pdf:pdf},
