journal = {Plos One},
keywords = {qol},
mendeley-tags = {qol},
number = {5},
pages = {e0127050},
pmid = {25965447},
title = {{Translation of Questionnaires Measuring Health Related Quality of Life Is Not Standardized: A Literature Based Research Study}},
url = {http://dx.plos.org/10.1371/journal.pone.0127050},
volume = {10},
year = {2015}
}
