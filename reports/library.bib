title = {{Bilateral Hip Arthroplasty: When Is It Safe to Operate the Second Hip? A Systematic Review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29682533 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5851297},
