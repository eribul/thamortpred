isbn = {0959-8138},
issn = {17561833},
journal = {BMJ (Online)},
number = {7880},
pages = {1--5},
pmid = {23097541},
title = {{How a fake hip showed up failings in European device regulation}},
volume = {345},
year = {2012}
}
